<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE suite SYSTEM "https://testng.org/testng-1.0.dtd">
<suite name="Aptem" verbose="3" parallel="tests" thread-count="2">
  
  
  <test name="AptemAutomation-TenantLevelVerifications-1" preserve-order="true">
		<classes>
			<class name="com.aptem.tests.TenantLevelTest">
				<methods>
					<include
						name="disableComplianceDocumentsAtTenantLevel">
						<parameter name="testcaseid" value="001" />
					</include>					
				</methods>
			</class>
		</classes>
	</test>
	<test name="AptemAutomation-TenantLevelVerifications-2" preserve-order="true">
		<classes>
			<class name="com.aptem.tests.TenantLevelTest">
				<methods>					
					 <include
						name="enableComplianceDocumentsAtTenantLevel">
						<parameter name="testcaseid" value="002" />
					</include>				
				</methods>
			</class>
		</classes>
	</test>
	<test name="AptemAutomation-TenantLevelVerifications-3" preserve-order="true">
		<classes>
			<class name="com.aptem.tests.TenantLevelTest">
				<methods>
					 <include
						name="addDocumentsToTheLearnersProgramme">
						<parameter name="testcaseid" value="003" />
					</include> 
				</methods>
			</class>
		</classes>
	</test>
</suite> 
